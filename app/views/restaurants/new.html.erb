<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.label :category %>
  <%= f.select(:category, ["chinese", "italian", "japanese", "french", "belgian"]) %>
  <%= f.submit "add a restaurant", class: "btn btn-primary" %>
<% end %>
<p><%= link_to "All restaurants",restaurants_path %></p>
